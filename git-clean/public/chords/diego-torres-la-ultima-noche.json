          "lines": [
            {
              "type": "chords",
              "content": ["Cm", "D"]
            },
            {
              "type": "lyric",
              "content": "Voy a quedarme solo, solo, solo solo y vivo"
            }
          ]
        },
        {
          "type": "voice",
          "name": "VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chords",
              "content": ["Cm", "D"]
            },
            {
              "type": "lyric",
              "content": "Dejando que se pierda poco a poco en el olvido"
            }
          ]
        }
      ]
