          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "Que brillan mis ojos hoy"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "G"
