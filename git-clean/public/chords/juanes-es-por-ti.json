              "content": "D"
            },
            {
              "type": "lyric",
              "content": "Que he vuelto a hablar de amor"
            }
          ]
        },
        {
          "type": "voice",
          "name": "2º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "Y es por ti"
            }
          ]
        },
        {
          "type": "voice",
          "name": "2º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "A"
            },
            {
              "type": "lyric",
              "content": "Que calma mi dolor"
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "SEGUNDA ESTROFA",
      "lines": [
        {
          "type": "chord",
          "content": "Bm"
        },
        {
          "type": "lyric",
          "content": "Y cada vez que yo te busco"
        },
        {
          "type": "chord",
          "content": "A"
        },
        {
          "type": "lyric",
          "content": "Y no te puedo aún hallar"
        },
        {
          "type": "chord",
          "content": "G"
        },
        {
          "type": "lyric",
          "content": "Me siento un vagabundo"
        },
        {
          "type": "chord",
          "content": "A"
        },
        {
          "type": "lyric",
          "content": "Perdido por el mundo"
        },
        {
          "type": "chord",
          "content": "Bm"
        },
        {
          "type": "lyric",
          "content": "Desordenado si no estás"
        },
        {
          "type": "chord",
          "content": "A"
        },
        {
          "type": "lyric",
          "content": "Como mueves tú mi felicidad"
        },
        {
          "type": "chord",
          "content": "G"
        },
        {
          "type": "lyric",
          "content": "Y todo lo que está de más"
        }
      ]
