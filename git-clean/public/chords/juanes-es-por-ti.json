{
  "id": "juanes-es-por-ti",
  "title": "Es Por Ti",
  "artist": "Juanes",
  "originalKey": "Bm",
  "tempo": "Moderate (Balada romántica)",
  "timeSignature": "4/4",
  "content": [
    {
      "type": "section",
      "name": "PRIMERA ESTROFA",
      "lines": [
        {
          "type": "chord",
          "content": "Bm"
        },
        {
          "type": "lyric",
          "content": "Cada vez que me levanto"
        },
        {
          "type": "chord",
          "content": "A"
        },
        {
          "type": "lyric",
          "content": "Y veo que a mi lado estás"
        },
        {
          "type": "chord",
          "content": "G"
        },
        {
          "type": "lyric",
          "content": "Me siento renovado"
        },
        {
          "type": "chord",
          "content": "A"
        },
        {
          "type": "lyric",
          "content": "Y me siento aniquilado"
        },
        {
          "type": "chord",
          "content": "Bm"
        },
        {
          "type": "lyric",
          "content": "Aniquilado si no estás"
        },
        {
          "type": "chord",
          "content": "A"
        },
        {
          "type": "lyric",
          "content": "Tú controlas toda mi verdad"
        },
        {
          "type": "chord",
          "content": "G"
        },
        {
          "type": "lyric",
          "content": "Y todo lo que está de más"
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "PRE-ESTRIBILLO",
      "lines": [
        {
          "type": "chord",
          "content": "G"
        },
        {
          "type": "lyric",
          "content": "Tus ojos me llevan lentamente al sol"
        },
        {
          "type": "chord",
          "content": "D/F#"
        },
        {
          "type": "lyric",
          "content": "Y tu boca me habla del amor y el corazón"
        },
        {
          "type": "chord",
          "content": "E"
        },
        {
          "type": "lyric",
          "content": "Tu piel tiene el color de un rojo atardecer"
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTRIBILLO",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "Y es por ti"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "Que late mi corazón"
            }
          ]
        },
        {
          "type": "voice",
          "name": "2º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "G"
