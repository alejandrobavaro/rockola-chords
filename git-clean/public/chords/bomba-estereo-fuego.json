            "color": "ale",
            "lines": [
              {
                "type": "lyric",
                "content": "Que aquí viene Bomba Estéreo"
              },
              {
                "type": "lyric",
                "content": "viene con too', chapeta"
              },
              {
                "type": "lyric",
                "content": "raggae music, cumbia y folclor ¡C'mon!"
              },
              {
                "type": "lyric",
                "content": "que es un poder que es una bomba atómica,"
              },
              {
                "type": "lyric",
                "content": "un poco de folclor con música electrónica."
              }
            ]
          }
        ]
      },
      {
        "type": "section",
        "name": "VERSO 2",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "lyric",
                "content": "¡Sigue sigue! prendiendo esta fiesta"
              },
              {
                "type": "lyric",
                "content": "¡sigue sigue! que a mi no me molesta"
              },
              {
                "type": "lyric",
                "content": "vamos a bailar, la noche y la madrugá"
              },
              {
                "type": "lyric",
                "content": "hasta que se te ponga, la pollera colorá."
              }
            ]
          }
        ]
      },
      {
        "type": "section",
        "name": "VERSO 3",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "lyric",
                "content": "Vamos hasta abajo a mi no me da pena"
              },
              {
                "type": "lyric",
                "content": "yo crecí tomando, agua de panela,"
              },
              {
                "type": "lyric",
                "content": "helada, y por allá de frío nada,"
              },
              {
                "type": "lyric",
                "content": "ahora te canto pa' que te des la calentada."
              }
            ]
          }
        ]
      },
