          "lines": [
            {
              "type": "chord",
              "content": "Bm7"
            },
            {
              "type": "lyric",
              "content": "Torn between the lover and the love you leave behind"
            },
            {
              "type": "chord",
              "content": "A"
            },
            {
              "type": "lyric",
              "content": "You're headed for disaster"
            }
