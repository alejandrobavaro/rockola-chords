    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "CUARTA PARTE",
      "lines": [
        {
          "type": "chords",
          "content": ["D", "G"]
        },
        {
          "type": "lyric",
          "content": "How would it be if you were standing in my shoes"
        },
        {
          "type": "chords",
          "content": ["D", "A/C#"]
        },
        {
          "type": "chords",
          "content": ["Bm", "A"]
        },
        {
          "type": "lyric",
          "content": "Can't you see that it's impossible to choose"
        },
        {
          "type": "chords",
          "content": ["G", "D/F#"]
        },
        {
          "type": "chords",
          "content": ["Em", "Bm"]
        },
        {
          "type": "lyric",
          "content": "No there's no making sense of it"
        },
        {
          "type": "chords",
          "content": ["Em", "Bm", "A", "A4", "A"]
        },
        {
          "type": "lyric",
          "content": "Every way I go I'm bound to lose, oh yeah"
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTRIBILLO (REPETICIÓN)",
