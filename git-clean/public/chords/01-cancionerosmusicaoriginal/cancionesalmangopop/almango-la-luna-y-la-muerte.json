          "color": "ale",
          "lines": [
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "La luna tiene dientes de marfil,"},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "es vieja y triste asoma!"},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "los cauces secos"},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "y los campos."}
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "Sin verdores"},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "y los árboles mustios,"},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "sin nidos y sin hojas."},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "Doña luna y la muerte."}
          ]
        }
      ]
    },
