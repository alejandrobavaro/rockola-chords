          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "La luna le ha comprado"},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "pinturas a la muerte."},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "En esta noche turbia"},
            {"type": "chord", "content": "Dm"},
