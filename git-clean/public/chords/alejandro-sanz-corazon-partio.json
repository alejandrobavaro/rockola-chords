          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "Em"
            },
            {
              "type": "lyric",
              "content": "¿Y quién me va a entregar sus emociones?"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "A7"
            },
            {
              "type": "lyric",
              "content": "¿Quién me va a pedir que nunca le abandone?"
            }
          ]
        },
        {
          "type": "voice",
          "name": "2º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "F#m"
            },
            {
              "type": "lyric",
              "content": "¿Quién me tapará esta noche si hace frío?"
            }
          ]
        },
        {
          "type": "voice",
          "name": "2º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "B7"
            },
            {
              "type": "lyric",
              "content": "¿Quién me va a curar el corazón partío?"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "Em"
            },
            {
              "type": "lyric",
              "content": "¿Quién llenará de primaveras este enero"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "A7"
            },
            {
              "type": "lyric",
              "content": "Y bajará la luna para que juguemos?"
            }
          ]
        },
        {
          "type": "voice",
          "name": "2º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "F#m"
            },
            {
              "type": "lyric",
              "content": "Dime, si tú te vas, dime, cariño mío"
