    {
      "type": "section",
      "name": "ESTRIBILLO 1",
      "lines": [
        {
          "type": "voice",
          "name": "VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chords",
              "content": ["G", "D"]
            },
            {
              "type": "lyric",
              "content": "Deja que tus sueños sean olas que se van"
            },
            {
              "type": "chords",
              "content": ["Em", "C", "D"]
            },
            {
              "type": "lyric",
              "content": "Libres como el viento en mitad del mar"
            }
          ]
        },
        {
          "type": "voice",
          "name": "VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chords",
              "content": ["G", "D"]
            },
            {
              "type": "lyric",
              "content": "Creo que la vida es un tesoro sin igual"
            },
            {
              "type": "chords",
              "content": ["Em", "C", "D", "G"]
            },
            {
              "type": "lyric",
              "content": "De los buenos tiempos siempre quiero más"
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "TERCER VERSO",
      "lines": [
        {
          "type": "voice",
          "name": "VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chords",
              "content": ["G", "Em"]
            },
            {
              "type": "lyric",
              "content": "Soy como el agua del río"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": "Y por el camino me dejo llevar"
            }
          ]
        },
        {
          "type": "voice",
          "name": "VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chords",
              "content": ["G", "Em"]
            },
            {
              "type": "lyric",
              "content": "Porque aprendí que la vida"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": "Por todo lo malo algo bueno te da"
