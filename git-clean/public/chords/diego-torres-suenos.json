{
  "id": "diego-torres-suenos",
  "title": "Sueños",
  "artist": "Diego Torres",
  "originalKey": "G",
  "tempo": "Moderate",
  "timeSignature": "4/4",
  "content": [
    {
      "type": "section",
      "name": "PRIMER VERSO",
      "lines": [
        {
          "type": "voice",
          "name": "VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chords",
              "content": ["G", "Em"]
            },
            {
              "type": "lyric",
              "content": "Cuando la noche se acerca"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": "Hay algo en mi alma que vuelve a vibrar"
            }
          ]
        },
        {
          "type": "voice",
          "name": "VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chords",
              "content": ["G", "Em"]
            },
            {
              "type": "lyric",
              "content": "Con la luz de las estrellas"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": "En mis sentimientos te vuelvo a encontrar"
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "PRECORO",
      "lines": [
        {
          "type": "voice",
          "name": "VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chords",
              "content": ["C", "D", "G"]
            },
            {
              "type": "lyric",
              "content": "Quiero que me mires a los ojos"
            },
            {
              "type": "chords",
              "content": ["C", "D", "G"]
            },
            {
              "type": "lyric",
              "content": "Y que no preguntes nada más"
            }
          ]
        },
        {
          "type": "voice",
          "name": "VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": "Quiero que esta noche sueltes"
            },
            {
              "type": "chords",
              "content": ["Em", "C", "D"]
            },
            {
              "type": "lyric",
              "content": "Toda esa alegría que ya no puedes guardar"
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "SEGUNDO VERSO",
      "lines": [
        {
          "type": "voice",
          "name": "VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chords",
              "content": ["G", "Em"]
            },
            {
              "type": "lyric",
              "content": "Paso las horas fumando"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": "Oyendo en el viento la misma canción"
            }
          ]
        },
        {
          "type": "voice",
          "name": "VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chords",
              "content": ["G", "Em"]
            },
            {
              "type": "lyric",
              "content": "Porque el tiempo que vale"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": "Lo marca el latido de mi corazón"
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
