{
  "id": "andres-calamaro-brindo",
  "title": "Brindo",
  "artist": "Andrés Calamaro",
  "originalKey": "G",
  "tempo": "Moderato",
  "timeSignature": "4/4",
  "content": [
    {
      "type": "section",
      "name": "ESTROFA 1",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "Brindo por las mujeres"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "que derrochan simpatía,"
            }
          ]
        },
        {
          "type": "chords",
          "content": ["D7"]
        },
        {
          "type": "lyric",
          "content": "brindo por los que vuelven"
        },
        {
          "type": "chords",
          "content": ["G"]
        },
        {
          "type": "lyric",
          "content": "con las luces de otro día."
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 2",
      "lines": [
        {
          "type": "chords",
          "content": ["G7"]
        },
        {
          "type": "lyric",
          "content": "Brindo porque recuerdo tu cuerpo,"
        },
        {
          "type": "chords",
          "content": ["C"]
        },
        {
          "type": "lyric",
          "content": "pero olvidé tu cara."
        },
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "C"
            },
            {
              "type": "lyric",
              "content": "Brindo por lo que tuve,"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "porque ya no tengo"
            },
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "nada."
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 3",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "Brindo por el momento"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "en que tú y yo nos conocimos,"
            }
          ]
        },
        {
          "type": "chords",
          "content": ["D7"]
        },
        {
          "type": "lyric",
          "content": "y por los corazones"
        },
        {
          "type": "chords",
          "content": ["G"]
        },
        {
          "type": "lyric",
          "content": "que se han roto en el camino."
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 4",
      "lines": [
        {
          "type": "chords",
          "content": ["G7"]
        },
        {
          "type": "lyric",
          "content": "Brindo por el recuerdo"
        },
        {
          "type": "chords",
          "content": ["C"]
        },
        {
          "type": "lyric",
          "content": "y también por el olvido,"
        },
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "C"
            },
            {
              "type": "lyric",
              "content": "brindo porque esta noche"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "un amigo paga"
            },
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "el vino..."
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "PUENTE 1",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "C"
            },
            {
              "type": "lyric",
              "content": "Desde un rincón del mundo..."
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "brindo contigo..."
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 5",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "Caiga quien caiga brindo"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "sobre la luz de una vela,"
            }
          ]
        },
        {
          "type": "chords",
          "content": ["D7"]
        },
        {
          "type": "lyric",
          "content": "toda la noche brindo"
        },
        {
          "type": "chords",
          "content": ["G"]
        },
        {
          "type": "lyric",
          "content": "y que la mañana venga."
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 6",
      "lines": [
        {
          "type": "chords",
          "content": ["G7"]
        },
        {
          "type": "lyric",
          "content": "No es un momento triste"
        },
        {
          "type": "chords",
          "content": ["C"]
        },
        {
          "type": "lyric",
          "content": "ya que brindo con amigos,"
        },
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "C"
            },
            {
              "type": "lyric",
              "content": "brindo por el futuro"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "con la noche de"
            },
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "testigo."
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 7",
      "lines": [
        {
          "type": "chords",
          "content": ["G", "G-D"]
        },
        {
          "type": "lyric",
          "content": "Si alguna vez no brindo"
        },
        {
          "type": "chords",
          "content": ["Em", "D"]
        },
        {
          "type": "lyric",
          "content": "siquiera por tonterías,"
        },
        {
          "type": "chords",
          "content": ["D7"]
        },
        {
          "type": "lyric",
          "content": "brindaré con silencio"
        },
        {
          "type": "chords",
          "content": ["G"]
        },
        {
          "type": "lyric",
          "content": "por la fortuna perdida."
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 8",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "Porque la vida es dura"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "por el fin de la amargura."
            }
          ]
        },
        {
          "type": "chords",
          "content": ["D7"]
        },
        {
          "type": "lyric",
          "content": "Brindo porque me olvido,"
        },
        {
          "type": "chords",
          "content": ["G"]
        },
        {
          "type": "lyric",
          "content": "los motivos porque brindo."
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 9",
      "lines": [
        {
          "type": "chords",
          "content": ["G7"]
        },
        {
          "type": "lyric",
          "content": "Brindo por lo que sea"
        },
        {
          "type": "chords",
          "content": ["C"]
        },
        {
          "type": "lyric",
          "content": "que caiga hoy en el vaso,"
        },
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "C"
            },
            {
              "type": "lyric",
              "content": "brindo por la victoria,"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "por el empate y"
            },
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "por el fracaso."
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 10",
      "lines": [
        {
          "type": "chords",
          "content": ["G"]
        },
        {
          "type": "lyric",
          "content": "Brindo por seguir"
        },
        {
          "type": "chords",
          "content": ["D", "Em", "D"]
        },
        {
          "type": "lyric",
          "content": "queriéndote toda la vida,"
        },
        {
          "type": "chords",
          "content": ["D7"]
        },
        {
          "type": "lyric",
          "content": "casi está lleno el vaso"
        },
        {
          "type": "chords",
          "content": ["G"]
        },
        {
          "type": "lyric",
          "content": "con la sangre de otra herida."
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 11",
      "lines": [
        {
          "type": "chords",
          "content": ["G7"]
        },
        {
          "type": "lyric",
          "content": "Brindo por la emoción"
        },
        {
          "type": "chords",
          "content": ["C"]
        },
        {
          "type": "lyric",
          "content": "pero también brindo por frialdad,"
        },
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "C"
            },
            {
              "type": "lyric",
              "content": "que la salud no falte"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "a toda la"
            },
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "humanidad..."
            }
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 12",
      "lines": [
        {
          "type": "chords",
          "content": ["G7"]
        },
        {
          "type": "lyric",
          "content": "Brindaré muy en serio"
        },
        {
          "type": "chords",
          "content": ["C"]
        },
        {
          "type": "lyric",
          "content": "por una vez en la vida,"
        },
        {
          "type": "chords",
          "content": ["C"]
        },
        {
          "type": "lyric",
          "content": "brindo hasta la cirrosis"
        },
        {
          "type": "chords",
          "content": ["D", "G"]
        },
        {
          "type": "lyric",
          "content": "por la vacuna del SIDA."
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "PUENTE FINAL",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ 1",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "C"
            },
            {
              "type": "lyric",
              "content": "Desde un rincón del mundo..."
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "brindo contigo..."
            },
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "¡SALUD!"
            }
          ]
        }
      ]
    }
  ]
}
