    "id": "axel-fernando-si-va-a-ser-sera",
    "title": "Si va a ser será",
    "artist": "Axel Fernando",
    "originalKey": "E",
    "tempo": "Moderate",
    "timeSignature": "4/4",
    "content": [
      {
        "type": "section",
        "name": "INTRO",
        "lines": [
          {
            "type": "chords",
            "content": ["E", "F#m", "G#m", "F#m", "E", "F#m", "B"]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "voice",
        "name": "VOZ 1",
        "color": "ale",
        "lines": [
          {
            "type": "chord",
            "content": "E"
          },
          {
            "type": "lyric",
            "content": "No me importa cuando"
          },
          {
            "type": "chord",
            "content": "F#m"
          },
          {
            "type": "lyric",
            "content": "ni como ni donde vaya a ser,"
          },
          {
            "type": "chord",
            "content": "B"
          },
          {
            "type": "lyric",
            "content": "no quisiera apurar al tiempo."
          }
        ]
      },
      {
        "type": "voice",
        "name": "VOZ 2",
        "color": "pato",
        "lines": [
          {
            "type": "chord",
            "content": "E"
          },
          {
            "type": "lyric",
            "content": "No me importa si estas sola,"
          },
          {
            "type": "chord",
            "content": "F#m"
          },
          {
            "type": "lyric",
            "content": "tampoco con quien,"
          },
          {
            "type": "chord",
            "content": "B"
          },
          {
            "type": "lyric",
            "content": "si va a ser, será en su momento."
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "PRE ESTRIBILLO",
        "lines": [
          {
            "type": "voice",
            "name": "VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "C#m"
              },
              {
                "type": "lyric",
                "content": "Y aunque muera por tu amor"
              },
              {
                "type": "chord",
                "content": "F#m"
              },
              {
                "type": "lyric",
                "content": "en este oscuro y profundo silencio,"
              }
            ]
          },
          {
            "type": "voice",
            "name": "VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "C#m"
              },
              {
                "type": "lyric",
                "content": "que se extiende hasta sentir que"
              },
              {
                "type": "chord",
                "content": "Am"
              },
              {
                "type": "lyric",
                "content": "estoy muriendo"
              },
              {
                "type": "chord",
                "content": "B"
              },
              {
                "type": "lyric",
                "content": "seguiré diciendo."
              }
            ]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTRIBILLO 1",
        "lines": [
          {
            "type": "voice",
            "name": "VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "F#m"
              },
              {
                "type": "lyric",
                "content": "Que no me importa"
              },
              {
                "type": "chord",
                "content": "B"
              },
              {
                "type": "lyric",
                "content": "si estas cerca o estas lejos,"
              }
            ]
          },
          {
            "type": "voice",
            "name": "VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "F#m"
              },
              {
                "type": "lyric",
                "content": "Descubrí que nuestro amor"
              },
              {
                "type": "chord",
                "content": "B"
              },
              {
                "type": "lyric",
                "content": "no tiene tiempos."
              }
            ]
          },
          {
            "type": "chords",
            "content": ["A", "G#"]
          },
          {
            "type": "lyric",
            "content": "Y no me importa tu pasado"
          },
          {
            "type": "chords",
            "content": ["C#m", "F#"]
          },
          {
            "type": "lyric",
            "content": "ni tu cuerpo."
          },
          {
            "type": "chords",
            "content": ["A", "B"]
          },
          {
            "type": "lyric",
            "content": "Por mil vidas yo te espero."
          }
        ]
      },
      {
        "type": "section",
        "name": "ESTRIBILLO 2",
