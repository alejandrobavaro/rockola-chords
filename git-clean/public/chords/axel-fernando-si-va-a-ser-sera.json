          {
            "type": "voice",
            "name": "VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "F#m"
              },
              {
                "type": "lyric",
                "content": "porque todo lo que siento"
              },
              {
                "type": "chord",
                "content": "B"
              },
              {
                "type": "lyric",
                "content": "pasa dentro."
              }
