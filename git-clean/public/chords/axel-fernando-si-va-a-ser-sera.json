      {
        "type": "voice",
        "name": "VOZ 1",
        "color": "ale",
        "lines": [
          {
            "type": "chord",
            "content": "E"
          },
          {
            "type": "lyric",
            "content": "Pero importa que comprendas"
          },
          {
            "type": "chord",
            "content": "F#m"
          },
          {
            "type": "lyric",
            "content": "todo tiene un porque"
          },
          {
            "type": "chord",
            "content": "B"
          },
          {
            "type": "lyric",
            "content": "y todo llegará y será eterno."
          }
        ]
      },
      {
        "type": "voice",
        "name": "VOZ 2",
        "color": "pato",
        "lines": [
          {
            "type": "chord",
            "content": "E"
          },
          {
            "type": "lyric",
            "content": "Cuando el tiempo nos encuentre"
          },
          {
            "type": "chord",
            "content": "F#m"
          },
          {
            "type": "lyric",
            "content": "se va a detener"
          },
          {
            "type": "chord",
            "content": "B"
          },
          {
            "type": "lyric",
            "content": "todo el universo en el momento."
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "voice",
        "name": "VOZ 1",
        "color": "ale",
        "lines": [
          {
            "type": "chord",
            "content": "C#m"
          },
          {
            "type": "lyric",
            "content": "Y hoy te espero con mi amor"
          },
          {
            "type": "chord",
            "content": "F#m"
          },
          {
            "type": "lyric",
            "content": "y ya no aguanto pero espero en silencio,"
          }
        ]
      },
      {
        "type": "voice",
        "name": "VOZ 2",
        "color": "pato",
        "lines": [
          {
            "type": "chord",
            "content": "C#m"
          },
