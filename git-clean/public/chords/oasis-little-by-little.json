        "type": "section",
        "name": "PUENTE",
        "lines": [
          {
            "type": "chords",
            "content": ["C"]
          },
          {
            "type": "lyric",
            "content": "You know I didn't mean,"
          },
          {
            "type": "chords",
            "content": ["F", "C/E", "Dm"]
          },
          {
            "type": "lyric",
            "content": "What I just said,"
          },
          {
            "type": "chords",
            "content": ["G"]
          },
          {
            "type": "lyric",
            "content": "But my god woke up on the wrong side of his bed;"
          },
          {
            "type": "chords",
            "content": ["F"]
          },
          {
            "type": "lyric",
            "content": "And it just don't matter now."
          }
        ]
