      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "PRIMERA PARTE",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "Te vi llegar del brazo de un amigo"
              },
              {
                "type": "chord",
                "content": "C#m7"
              },
              {
                "type": "lyric",
                "content": "Cuando entraste al bar y te caíste"
              },
              {
                "type": "chord",
                "content": "Cm7"
              },
              {
                "type": "chord",
                "content": "Bm7"
              },
              {
                "type": "lyric",
                "content": "Al piso me tiraste el pingüino"
              },
              {
                "type": "lyric",
                "content": "Me tiraste el sifón"
              },
              {
                "type": "chord",
                "content": "E7"
              },
              {
                "type": "lyric",
                "content": "Estallaron los vidrios de mi corazón"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "Te vi bailar, brillando con tu ausencia"
              },
              {
                "type": "chord",
                "content": "C#m7"
              },
              {
                "type": "lyric",
                "content": "Sin sentir piedad, chocando con las mesas"
              },
              {
                "type": "chord",
                "content": "Cm7"
              },
              {
                "type": "chord",
                "content": "Bm7"
              },
              {
                "type": "lyric",
                "content": "Te burlaste de todos, te reíste de mí"
              },
              {
                "type": "chord",
                "content": "E7"
              },
              {
                "type": "lyric",
                "content": "Tus amigos se escaparon de vos"
              }
            ]
          }
        ]
      },
