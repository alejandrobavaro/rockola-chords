            },
            {
              "type": "lyric",
              "content": "I've found a reason for me"
            },
            {
              "type": "chords",
              "content": ["F"]
            },
            {
              "type": "lyric",
              "content": "To change who I used to be"
            },
            {
              "type": "chords",
              "content": ["G"]
            },
            {
              "type": "lyric",
              "content": "A reason to start over new"
            },
            {
              "type": "chords",
              "content": ["C"]
            },
            {
              "type": "lyric",
              "content": "And the reason is you"
            }
          ]
