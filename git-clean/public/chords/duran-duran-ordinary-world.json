          "lines": [
            {
              "type": "chords",
              "content": ["Am"]
            },
            {
              "type": "lyric",
              "content": "Came in from a rainy Thursday on the avenue"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": ""
            },
            {
              "type": "chords",
              "content": ["Am"]
            },
            {
              "type": "lyric",
              "content": "Thought I heard you talking softly"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": ""
            },
            {
              "type": "chords",
              "content": ["Am"]
            },
            {
              "type": "lyric",
              "content": "I turned on the lights, the TV and the radio"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": ""
            },
            {
              "type": "chords",
              "content": ["Am"]
            },
            {
              "type": "lyric",
              "content": "Still I can't escape the ghost of you"
            },
            {
              "type": "chords",
              "content": ["C", "D"]
            },
            {
              "type": "lyric",
              "content": ""
            },
            {
              "type": "chords",
              "content": ["Am7"]
            },
            {
              "type": "lyric",
              "content": "What has happened to it all?"
            },
            {
              "type": "chords",
              "content": ["Em", "Em9"]
            },
            {
              "type": "lyric",
              "content": "Crazy, some'd say"
            },
            {
              "type": "chords",
              "content": ["B7", "C"]
            },
            {
              "type": "lyric",
              "content": "Where is the life that I recognize? Gone away"
            }
          ]
        }
      ]
    },
    {
