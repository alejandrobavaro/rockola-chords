        "type": "section",
        "name": "PUENTE FINAL",
        "lines": [
          {
            "type": "chords",
            "content": ["C", "G"]
          },
          {
            "type": "lyric",
            "content": "Y que vengan ríos de bondad,"
          },
          {
            "type": "chords",
            "content": ["E", "Am"]
          },
          {
            "type": "lyric",
            "content": "a todos los pueblos de la tierra"
          },
          {
            "type": "chords",
            "content": ["F", "C"]
          },
          {
            "type": "lyric",
            "content": "que no nos podemos olvidar."
          },
          {
            "type": "chords",
            "content": ["Dm", "Am"]
          },
          {
            "type": "lyric",
            "content": "Que el amor puro libera,"
          },
          {
            "type": "chords",
            "content": ["E", "Am"]
          },
          {
            "type": "lyric",
            "content": "y la mentira envenena."
          },
