{
    "id": "ricky-martin-pegate",
    "title": "Pégate",
    "artist": "Ricky Martin",
    "originalKey": "Am",
    "tempo": "Allegro",
    "timeSignature": "4/4",
    "content": [
      {
        "type": "section",
        "name": "INTRO TROMPETAS",
        "lines": [
          {
            "type": "chords",
            "content": ["Am", "E", "E", "Am", "Am", "E", "E", "Am"]
          },
          {
            "type": "text",
            "content": "(Repetir 2 veces)"
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "voice",
        "name": "1º VOZ 1",
        "color": "ale",
        "lines": [
          {
            "type": "chord",
            "content": "Am"
          },
          {
            "type": "lyric",
            "content": "Le lo lai, ay lelolelo, yo vengo con cosa buena"
          },
          {
            "type": "chord",
            "content": "E"
          },
          {
            "type": "lyric",
            "content": "para mi pueblo, traigo amor, traigo ese suero,"
          },
          {
            "type": "chord",
            "content": "E"
          },
          {
            "type": "lyric",
            "content": "que alegra los corazones del mundo entero."
          }
        ]
      },
      {
        "type": "voice",
        "name": "1º VOZ 2",
        "color": "pato",
        "lines": [
          {
            "type": "chord",
            "content": "Am"
          },
          {
            "type": "lyric",
            "content": "Pa'l dolor, pa'l mal de amores,"
          },
          {
            "type": "chord",
            "content": "A7"
          },
          {
            "type": "lyric",
            "content": "nada como el repique de mis tambores."
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "voice",
        "name": "1º VOZ 1",
        "color": "ale",
        "lines": [
          {
            "type": "chord",
            "content": "Dm"
          },
          {
            "type": "lyric",
            "content": "Que qué hay, tirarse a la calle,"
          },
          {
            "type": "chord",
            "content": "Am"
          },
          {
            "type": "lyric",
            "content": "dejando atrás los problemas."
          }
        ]
      },
      {
        "type": "voice",
        "name": "1º VOZ 2",
        "color": "pato",
        "lines": [
          {
            "type": "chord",
            "content": "Dm"
          },
          {
            "type": "lyric",
            "content": "Que como decía mi madre,"
          },
          {
            "type": "chord",
            "content": "Am"
          },
          {
            "type": "lyric",
            "content": "bailando todo se arregla."
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTRIBILLO",
        "lines": [
          {
            "type": "chords",
            "content": ["Am", "E"]
          },
          {
            "type": "lyric",
            "content": "Pégate un poco más, pegado a los tambores."
          },
          {
            "type": "chords",
            "content": ["E", "Am"]
          },
          {
            "type": "lyric",
            "content": "Olvida los temores, que el tiempo se nos va."
          },
          {
            "type": "chords",
            "content": ["Am", "E"]
          },
          {
            "type": "lyric",
            "content": "Pégate un poco más, y mueve esas caderas"
          },
          {
            "type": "chords",
            "content": ["E", "Am"]
          },
          {
            "type": "lyric",
            "content": "Mamita cosa buena, que a mí me pone mal."
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "INTRO TROMPETAS (REPETICIÓN)",
        "lines": [
          {
            "type": "chords",
            "content": ["Am", "E", "E", "Am", "Am", "E", "E", "Am"]
          },
          {
            "type": "text",
            "content": "(Repetir 2 veces)"
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "voice",
        "name": "1º VOZ 1",
        "color": "ale",
        "lines": [
          {
            "type": "chord",
            "content": "Am"
          },
          {
            "type": "lyric",
            "content": "Mueve tus caderas, muchacha morena."
          },
          {
            "type": "chord",
            "content": "E"
          },
          {
            "type": "lyric",
            "content": "Báilame ese ritmo con sabor a pena."
          }
        ]
      },
      {
        "type": "voice",
        "name": "1º VOZ 2",
        "color": "pato",
        "lines": [
          {
            "type": "chord",
            "content": "Am"
          },
          {
            "type": "lyric",
            "content": "Ay, una pesetita pa' esa vellonera,"
          },
          {
            "type": "chord",
            "content": "E"
          },
          {
            "type": "lyric",
            "content": "para que te olvides de todas tus penas."
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "PUENTE",
        "lines": [
          {
            "type": "chords",
            "content": ["Am"]
          },
          {
            "type": "lyric",
            "content": "Y esta noche quiero más,"
          },
          {
            "type": "chords",
            "content": ["E"]
          },
          {
            "type": "lyric",
            "content": "y esta noche quiero fiesta, ieeeh."
          },
          {
            "type": "chords",
            "content": ["Am", "G"]
          },
          {
            "type": "lyric",
            "content": "Hoy no habrá mal que por bien no venga,"
          },
          {
            "type": "chords",
            "content": ["C", "G"]
          },
          {
            "type": "lyric",
            "content": "unamos los corazones,"
          },
          {
            "type": "chords",
            "content": ["E", "Am"]
          },
          {
            "type": "lyric",
            "content": "hoy todos somos multicolores."
          }
        ]
