      {
        "type": "section",
        "name": "BIS",
        "lines": [
          {
            "type": "chords",
            "content": ["Am"]
          },
          {
            "type": "lyric",
            "content": "Y que venga el coro, que venga"
          },
          {
            "type": "lyric",
            "content": "con todo el amor, que venga"
          },
          {
            "type": "chords",
            "content": ["E"]
          },
          {
            "type": "lyric",
            "content": "para nuestros niños, que venga"
          },
          {
            "type": "lyric",
            "content": "que venga la paz, que venga"
          },
          {
            "type": "chords",
            "content": ["Am"]
          },
          {
            "type": "lyric",
            "content": "que vengan todos, que vengan"
          },
          {
            "type": "lyric",
            "content": "a bailar mi plena, que vengan"
          },
          {
            "type": "chords",
            "content": ["E"]
          },
          {
            "type": "lyric",
            "content": "ahí bien pegaditos, que vengan"
          },
          {
            "type": "lyric",
            "content": "con mucho cariñito, que vengan."
          },
          {
            "type": "text",
            "content": "(Arreglo: E-F-F#-G-G#-A#)"
          }
        ]
      },
      {
        "type": "divider"
