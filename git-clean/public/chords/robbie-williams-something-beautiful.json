  "content": [
    {
      "type": "section",
      "name": "PRIMERA PARTE",
      "lines": [
        {
          "type": "chords",
          "content": ["G", "Em"]
        },
        {
          "type": "lyric",
          "content": "You can't manufacture a miracle"
        },
        {
          "type": "chords",
          "content": ["Bm", "C"]
        },
        {
          "type": "lyric",
          "content": "The silence was pitiful that day"
        },
        {
          "type": "chords",
          "content": ["G", "Em"]
        },
        {
          "type": "lyric",
          "content": "And love is getting too cynical"
        },
        {
          "type": "chords",
          "content": ["Bm", "D"]
        },
        {
          "type": "lyric",
          "content": "Passion's just physical these days"
        },
        {
          "type": "chords",
          "content": ["C", "Cm"]
        },
        {
          "type": "lyric",
          "content": "You analyse everyone you meet"
        },
        {
          "type": "chords",
          "content": ["G", "G7"]
        },
        {
          "type": "lyric",
          "content": "But get no sign, loving kind"
        },
        {
          "type": "chords",
          "content": ["C", "A"]
        },
        {
          "type": "lyric",
          "content": "Every night you admit defeat"
        },
        {
          "type": "chords",
          "content": ["D"]
        },
        {
          "type": "lyric",
          "content": "And cry yourself blind"
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
