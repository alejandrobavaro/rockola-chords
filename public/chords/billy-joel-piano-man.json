          "color": "ale",
          "lines": [
            {
              "type": "chords",
              "content": ["C", "G/B", "Am", "Am/G"]
            },
            {
              "type": "lyric",
              "content": "It's nine o'clock on a Saturday"
            },
            {
              "type": "chords",
              "content": ["F", "C/E", "D7", "G"]
            },
            {
              "type": "lyric",
              "content": "The regular crowd shuffles in"
            },
            {
              "type": "chords",
              "content": ["C", "G/B", "Am", "Am/G"]
            },
            {
              "type": "lyric",
              "content": "There's an old man sitting next to me"
            },
            {
              "type": "chords",
              "content": ["F", "G11", "C", "G/B"]
            },
            {
              "type": "lyric",
              "content": "Makin' love to his tonic and gin"
            },
            {
              "type": "chords",
              "content": ["C", "G/B", "Am", "Am/G"]
            },
            {
              "type": "lyric",
              "content": "He says, 'Son, can you play me a memory'"
            },
            {
              "type": "chords",
              "content": ["F", "C/E", "D7", "G"]
            },
            {
              "type": "lyric",
              "content": "I'm not really sure how it goes"
            },
            {
              "type": "chords",
              "content": ["C", "G/B", "Am"]
            },
            {
              "type": "lyric",
              "content": "But it's sad and it's sweet and I knew it"
            },
            {
              "type": "chords",
              "content": ["Am/G"]
            },
            {
              "type": "lyric",
              "content": "complete"
            },
            {
              "type": "chords",
              "content": ["F", "G11", "C"]
            },
            {
              "type": "lyric",
              "content": "When I wore a younger man's clothes'"
            }
