    {
      "type": "section",
      "name": "VERSO 2",
      "lines": [
        {
          "type": "chords",
          "content": ["Bm7", "Asus4"]
        },
        {
          "type": "lyric",
          "content": "I know from the outside"
        },
        {
          "type": "chords",
          "content": ["A", "Asus4", "A"]
        },
        {
          "type": "lyric",
          "content": "We looked good for each other"
        },
        {
          "type": "chords",
          "content": ["Bm7", "Asus4"]
        },
        {
          "type": "lyric",
          "content": "I guess things were going wrong when"
        },
        {
          "type": "chords",
          "content": ["A", "Asus4", "A"]
        },
        {
          "type": "lyric",
          "content": "You didn't like my mother"
        },
        {
          "type": "chords",
          "content": ["F#m", "F#", "Em7"]
        },
        {
          "type": "lyric",
          "content": "I don't want to hate but that's all you've left me with"
        },
        {
          "type": "chords",
          "content": ["Gmaj7"]
        },
        {
          "type": "lyric",
          "content": ""
        },
        {
          "type": "chords",
          "content": ["F#m", "F#", "Em7"]
        },
        {
          "type": "lyric",
          "content": "A bitter after taste and a fantasy"
        },
        {
          "type": "chords",
          "content": ["A"]
        },
        {
          "type": "lyric",
          "content": "Of how we all can live"
        }
      ]
