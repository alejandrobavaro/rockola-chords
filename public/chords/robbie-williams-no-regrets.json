    {
      "type": "section",
      "name": "VERSO 1",
      "lines": [
        {
          "type": "chords",
          "content": ["Bm7", "Asus4"]
        },
        {
          "type": "lyric",
          "content": "Tell me a story"
        },
        {
          "type": "chords",
          "content": ["A"]
        },
        {
          "type": "lyric",
          "content": "Where we all changed"
        },
        {
          "type": "chords",
          "content": ["Bm7", "Asus4"]
        },
        {
          "type": "lyric",
          "content": "And we'd live our lives together"
        },
        {
          "type": "chords",
          "content": ["A"]
        },
        {
          "type": "lyric",
          "content": "And not estranged"
        },
        {
          "type": "chords",
          "content": ["F#m", "F#", "Em7"]
        },
        {
          "type": "lyric",
          "content": "I didn't lose my mind, it was mine to give away"
        },
        {
          "type": "chords",
          "content": ["Gmaj7"]
        },
        {
          "type": "lyric",
          "content": ""
        },
        {
          "type": "chords",
          "content": ["F#m", "F#", "Em7"]
        },
        {
          "type": "lyric",
          "content": "Couldn't stay to watch me cry, you didn't have the time"
        },
        {
          "type": "chords",
          "content": ["A"]
        },
        {
          "type": "lyric",
          "content": "So I softly slip away"
        }
      ]
    },
