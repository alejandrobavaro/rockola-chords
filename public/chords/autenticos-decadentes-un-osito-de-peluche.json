      {
        "type": "section",
        "name": "INTRO",
        "lines": [
          {
            "type": "chords",
            "content": ["D", "G", "D", "G"]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "PRIMERA PARTE",
        "lines": [
          {
            "type": "voice",
            "name": "VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chords",
                "content": ["D", "G"]
              },
              {
                "type": "lyric",
                "content": "Dentro de tus ojos veo un lago"
              },
              {
                "type": "chord",
                "content": "D"
              },
              {
                "type": "lyric",
                "content": "Donde un hada se desnuda"
              },
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "Para que la adore el Sol"
              }
            ]
          },
          {
            "type": "voice",
            "name": "VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chords",
                "content": ["D", "G"]
              },
              {
                "type": "lyric",
                "content": "La melancolía de la tarde"
              },
              {
                "type": "chord",
                "content": "D"
              },
              {
                "type": "lyric",
                "content": "Me ha ganado el corazón"
              },
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "Y se nubla de dudas"
              }
            ]
          },
          {
            "type": "voice",
            "name": "VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "D"
              },
              {
                "type": "lyric",
                "content": "Son esos momentos"
              },
              {
                "type": "chords",
                "content": ["G", "D"]
              },
              {
                "type": "lyric",
                "content": "En que uno se pone a reflexionar"
              },
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "Y alumbra una tormenta"
              }
            ]
          },
          {
            "type": "voice",
            "name": "VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "D"
              },
              {
                "type": "lyric",
                "content": "Todo es tan tranquilo"
              },
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "Que el silencio anuncia el ruido"
              },
              {
                "type": "chords",
                "content": ["D", "G"]
              },
              {
                "type": "lyric",
                "content": "De la calma que antecede al huracán"
              }
            ]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTRIBILLO",
        "lines": [
          {
            "type": "voice",
            "name": "VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chords",
                "content": ["D", "Bm"]
              },
              {
                "type": "lyric",
                "content": "De repente no puedo respirar"
              },
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "Necesito un poco de libertad"
              },
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "Que te alejes por un tiempo de mi lado"
              },
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "Que me dejes en paz"
              }
            ]
          },
          {
            "type": "voice",
            "name": "VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chords",
                "content": ["D", "Bm"]
              },
              {
                "type": "lyric",
                "content": "Siempre fue mi manera de ser"
              },
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "No me trates de comprender"
              },
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "No hay nada que se pueda hacer"
              },
              {
                "type": "lyric",
                "content": "Soy un poco paranoico lo siento"
              }
            ]
          },
          {
            "type": "voice",
            "name": "VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chords",
                "content": ["D", "Bm"]
              },
              {
                "type": "lyric",
                "content": "Al ratito ya te empiezo a extrañar"
              },
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "Me preocupa que te pueda perder"
              },
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "Necesito que te acerques a mi"
              },
              {
                "type": "lyric",
                "content": "Para sentir el calor de tu cuerpo"
              }
            ]
          },
          {
            "type": "voice",
            "name": "VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chords",
                "content": ["D", "Bm"]
              },
              {
                "type": "lyric",
                "content": "Un osito de peluche de Taiwán"
              },
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "Una cáscara de nuez en el Mar"
              },
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "Suavecito como alfombra de piel"
              },
              {
                "type": "lyric",
                "content": "Delicioso como el dulce de leche"
              }
            ]
          }
        ]
