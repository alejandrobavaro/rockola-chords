{
    "id": "enanitos-verdes-tu-carcel",
    "title": "Tu Cárcel",
    "artist": "Enanitos Verdes",
    "originalKey": "D",
    "tempo": "Moderato",
    "timeSignature": "4/4",
    "content": [
      {
        "type": "section",
        "name": "INTRO",
        "lines": [
          {
            "type": "chords",
            "content": ["D", "A", "Bm", "G", "A"]
          },
          {
            "type": "text",
            "content": "(Repetir 2 veces)"
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTROFA 1",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "D"
              },
              {
                "type": "lyric",
                "content": "Te vas amor, si así lo quieres"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "¿qué puedo yo hacer?"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "Bm"
              },
              {
                "type": "lyric",
                "content": "Tu vanidad no te deja entender"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "que en la pobreza se sabe querer."
              }
            ]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTROFA 2",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "D"
              },
              {
                "type": "lyric",
                "content": "Quiero llorar y me destroza"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "que pienses así"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "Bm"
              },
              {
                "type": "lyric",
                "content": "Y más que ahora me quede sin ti"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "me duele lo que tú vas a sufrir."
              }
            ]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTRIBILLO",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "D"
              },
              {
                "type": "lyric",
                "content": "Pero recuerda, nadie es perfecto"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "y tú lo verás"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "Bm"
              },
              {
                "type": "lyric",
                "content": "Más de mil cosas mejores tendrás"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "pero cariño sincero, jamás."
              }
            ]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTROFA 3",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "D"
              },
              {
                "type": "lyric",
                "content": "Vete olvidando de esto que hoy dejas"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "y que cambiarás"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "Bm"
              },
              {
                "type": "lyric",
                "content": "Por la aventura que tú ya verás"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "será tu cárcel y nunca saldrás."
              }
            ]
          }
        ]
