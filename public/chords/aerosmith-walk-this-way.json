          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "C7"
            },
            {
              "type": "lyric",
              "content": "'Cause the best things in lovin'"
            },
            {
              "type": "chord",
              "content": "C7"
            },
            {
              "type": "lyric",
              "content": "With her sister and her cousin"
            },
            {
              "type": "chord",
              "content": "C7"
            },
            {
              "type": "lyric",
              "content": "Only started with a little kiss, like this!"
