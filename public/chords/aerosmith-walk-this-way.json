          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "C7"
            },
            {
              "type": "lyric",
              "content": "'Til I talked to my daddy, he say"
            },
            {
              "type": "chord",
              "content": "C7"
            },
            {
              "type": "lyric",
              "content": "He said you ain't seen nothing"
