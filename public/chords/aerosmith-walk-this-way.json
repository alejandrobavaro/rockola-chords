        },
        {
          "type": "voice",
          "name": "VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "C7"
            },
            {
              "type": "lyric",
              "content": "I met a cheerleader, was a real young bleeder"
            },
            {
              "type": "chord",
              "content": "C7"
            },
            {
              "type": "lyric",
              "content": "All the times I could reminisce"
