          "lines": [
            {
              "type": "chords",
              "content": ["C", "D9(11)"]
            },
            {
              "type": "lyric",
              "content": "I used to rule the world"
            },
            {
              "type": "chords",
              "content": ["G", "Em"]
            },
            {
              "type": "lyric",
              "content": "Seas would rise when I gave the word"
