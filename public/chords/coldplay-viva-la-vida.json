          "lines": [
            {
              "type": "chords",
              "content": ["C", "D9(11)"]
            },
            {
              "type": "lyric",
              "content": "I used to roll the dice"
            },
            {
              "type": "chords",
              "content": ["G", "Em"]
            },
            {
              "type": "lyric",
              "content": "Feel the fear in my enemy's eyes"
            }
