            {
              "type": "chords",
              "content": ["C", "D9(11)"]
            },
            {
              "type": "lyric",
              "content": "Now in the morning and I sleep alone"
            },
            {
              "type": "chords",
              "content": ["G", "Em"]
            },
            {
              "type": "lyric",
              "content": "Sweep the streets I used to own"
            }
          ]
        },
        {
          "type": "chords",
          "content": ["C", "D9(11)", "G", "Em"]
        },
        {
