          "name": "VOZ 2",
          "color": "pato",
          "lines": [
            {
              "type": "chords",
              "content": ["C", "D9(11)"]
            },
            {
              "type": "lyric",
              "content": "Listen as the crowd would sing"
            },
            {
              "type": "chord",
              "content": "G"
            },
            {
              "type": "lyric",
              "content": "Now the old king is dead!"
            },
