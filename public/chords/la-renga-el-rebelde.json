              {
                "type": "chord",
                "content": "E"
              },
              {
                "type": "lyric",
                "content": "por ahí he de andar"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "B"
              },
              {
                "type": "lyric",
                "content": "buscándome un rumbo,"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "B"
              },
              {
                "type": "lyric",
                "content": "ser socio de esta sociedad"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "me puede matar."
              },
              {
                "type": "chord",
                "content": "E"
              },
              {
                "type": "lyric",
                "content": ""
              }
            ]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTROFA 5",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "E"
              },
              {
                "type": "lyric",
                "content": "Yo veo todo al revés, no veo como usted"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "B"
              },
              {
                "type": "lyric",
                "content": "yo no veo justicia, sólo miseria y hambre"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "o será que soy yo que llevo la contra"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "E"
              },
              {
                "type": "lyric",
                "content": "como estandarte."
              }
            ]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTROFA 6",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "E"
              },
              {
                "type": "lyric",
                "content": "Perdónenme pero soy así soy,"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "E"
              },
              {
                "type": "lyric",
                "content": "yo no sé por qué"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "B"
              },
              {
                "type": "lyric",
                "content": "sé que hay otros también,"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "B"
              },
              {
                "type": "lyric",
                "content": "es que alguien debía de serlo,"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 1",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "A"
              },
              {
                "type": "lyric",
                "content": "que prefiera la rebelión,"
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ 2",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "E"
              },
              {
                "type": "lyric",
                "content": "a vivir padeciendo."
              }
            ]
          }
        ]
      },
      {
        "type": "divider"
      },
